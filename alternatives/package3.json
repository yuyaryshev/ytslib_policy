{
    "name": "yatasks_one_api",
    "version": "1.0.0",
    "type": "module",
    "private": true,
    "description": "Load issues from Jira and post them to Oracle DB",
    "main": "index.js",
    "sideEffects_DISABLED": [],
    "scripts": {
        "start": "node ts_out\\src\\start.js",
        "test": "jest",
        "watch": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline -w",
        "watch:compile:api": "babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline -w",
        "watch:restart:api": "nodemon ts_out\\src\\start.js --devuser=dev",
        "web_server": "webpack --config webpack.react.config.js --watch",
        "web_server_dev": "webpack-cli serve --mode development --config webpack.react.config.js",
        "build:frontend": "inprint & webpack-cli --mode production --config webpack.react.config.js",
        "build:api": "inprint & babel src --config-file ./babel.ts.config.js --out-dir ts_out/src --extensions \".ts,.tsx\" --source-maps inline",
        "build": "npm run build:api & npm run build:frontend",
        "ycplmon": "node ..\\ycplmon_amaglion\\ts_out\\src\\ycplmon.js src",
        "tsc": "tsc",
        "deps": "echo Finding loops in .js requires... & del deps.png & madge ts_out/src -c -i deps.png && deps.png",
        "deps_all": "echo Generating full .js requires tree... & del deps.png & madge ts_out/src -i deps.png && deps.png",
        "deps_orphans": "echo Finding orphans .js requires... & del deps.png & madge ts_out/src --orphans -i deps.png && deps.png",
        "deploy": "pm2 stop yatasks & git checkout . && git pull && pnpm i && npm run build && pm2 start yatasks"
    },
